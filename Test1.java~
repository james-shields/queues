public class Test1 {
  
  public static void main(String[] args) {
    Deque<String> d = new Deque<String>();
    
    d.addFirst("Hello");
    d.addLast("world");
    
    System.out.println(d.removeLast());
    System.out.println(d.removeFirst());
    
    d.addFirst("you");
    d.addFirst("are");
    
    System.out.println(d.removeFirst());
    System.out.println(d.removeLast());
    System.out.println(d.size());
  }
  
}